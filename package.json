		"css": "^3.0.0",
		"postcss": "^7.0.35",
		"tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
		"vue": "^2.6.12",
		"vue-meta": "^2.4.0",
		"vue-router": "^3.4.5"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.5.10",
		"@vue/cli-plugin-router": "^4.5.10",
		"@vue/cli-service": "~4.5.10",
		"eslint": "^7.18.0",
		"eslint-plugin-vue": "^7.4.1",
		"postcss-cli": "^7.0.35",
